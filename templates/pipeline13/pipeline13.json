{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "yujiangit"
		},
		"linkedService2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedService2'"
		},
		"AzureBlob1c_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "y"
		},
		"AzureBlob1c_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "x"
		},
		"linkedService2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "xx"
		},
		"pipeline13_properties_activities_2_typeProperties_activities_1_linkedServiceName": {
			"type": "string",
			"metadata": {
				"linkedServiceType": "HDInsight"
			}
		},
		"AzureBlob1c_properties_linkedServiceName": {
			"type": "string",
			"metadata": {
				"linkedServiceType": "AzureBlobStorage"
			}
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline13')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Web1",
						"description": "web activity description abc",
						"type": "WebActivity",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false
						},
						"typeProperties": {
							"url": "xcv",
							"method": "POST"
						}
					},
					{
						"name": "Copy Data1",
						"description": "copy activity description  cbd",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false
						},
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0
						},
						"inputs": [
							{
								"referenceName": "AzureBlob1c",
								"type": "DatasetReference"
							}
						]
					},
					{
						"name": "ForEach1",
						"type": "ForEach",
						"typeProperties": {
							"activities": [
								{
									"name": "If Condition1",
									"type": "IfCondition"
								},
								{
									"name": "Hive1",
									"type": "HDInsightHive",
									"policy": {
										"timeout": "7.00:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"typeProperties": {
										"scriptPath": ""
									},
									"linkedServiceName": {
										"referenceName": "[parameters('pipeline13_properties_activities_2_typeProperties_activities_1_linkedServiceName')]",
										"type": "LinkedServiceReference"
									}
								}
							]
						}
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureBlob1c')]",
				"[concat(variables('factoryId'), '/linkedServices/linkedService2')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlob1c')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "[parameters('AzureBlob1c_properties_linkedServiceName')]",
					"type": "LinkedServiceReference"
				},
				"type": "AzureBlob",
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"fileName": "[parameters('AzureBlob1c_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('AzureBlob1c_properties_typeProperties_folderPath')]"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/linkedService2')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "HDInsight",
				"typeProperties": {
					"clusterUri": "http://xxx",
					"userName": "[parameters('linkedService2_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "**********"
					},
					"linkedServiceName": {
						"referenceName": "AzureBlobStorage1",
						"type": "LinkedServiceReference"
					}
				}
			}
		}
	]
}