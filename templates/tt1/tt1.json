{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "yujiangit"
		},
		"AzureBlob1c_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "y"
		},
		"AzureBlob1c_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "x"
		},
		"AmazonS3Object1_properties_linkedServiceName": {
			"type": "string",
			"metadata": {
				"linkedServiceType": "AmazonS3"
			}
		},
		"AzureBlob1c_properties_linkedServiceName": {
			"type": "string",
			"metadata": {
				"linkedServiceType": "AzureBlobStorage"
			}
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline132')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data1",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "FileSystemSource",
								"recursive": true
							},
							"sink": {
								"type": "BlobSink"
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0
						},
						"inputs": [
							{
								"referenceName": "AmazonS3Object1",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "AzureBlob1c",
								"type": "DatasetReference"
							}
						]
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AmazonS3Object1')]",
				"[concat(variables('factoryId'), '/datasets/AzureBlob1c')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AmazonS3Object1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": "[parameters('AmazonS3Object1_properties_linkedServiceName')]",
				"folder": {
					"name": "xxxx"
				},
				"type": "AmazonS3Object",
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"key": "klu",
					"prefix": "",
					"bucketName": "xc"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlob1c')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": "[parameters('AzureBlob1c_properties_linkedServiceName')]",
				"type": "AzureBlob",
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"fileName": "[parameters('AzureBlob1c_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('AzureBlob1c_properties_typeProperties_folderPath')]"
				}
			}
		}
	]
}