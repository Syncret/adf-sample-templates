{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "yujiangit"
		},
		"AzureBlob1c3_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "y"
		},
		"AzureBlob1c3_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "x"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline132')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Web1",
						"description": "web activity description abc",
						"type": "WebActivity",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false
						},
						"typeProperties": {
							"url": "xcv",
							"method": "POST"
						}
					},
					{
						"name": "Copy Data1",
						"description": "copy activity description  cbd",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false
						},
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0
						},
						"inputs": [
							{
								"referenceName": "AzureBlob1c3",
								"type": "DatasetReference"
							}
						]
					},
					{
						"name": "Get Metadata1",
						"type": "GetMetadata",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"dataset": {
								"referenceName": "AmazonS3Object1",
								"type": "DatasetReference"
							},
							"fieldList": [
								"columnCount",
								"lastModified",
								{
									"value": "@pipeline().DataFactory",
									"type": "Expression"
								}
							]
						}
					},
					{
						"name": "ForEach1",
						"type": "ForEach",
						"typeProperties": {
							"activities": [
								{
									"name": "Copy Data1",
									"type": "Copy",
									"policy": {
										"timeout": "7.00:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"typeProperties": {
										"enableStaging": false,
										"dataIntegrationUnits": 0
									}
								},
								{
									"name": "If Condition1",
									"type": "IfCondition",
									"typeProperties": {
										"ifTrueActivities": [
											{
												"name": "Copy Data1",
												"type": "Copy",
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"typeProperties": {
													"enableStaging": false,
													"dataIntegrationUnits": 0
												}
											}
										]
									}
								}
							]
						}
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureBlob1c3')]",
				"[concat(variables('factoryId'), '/datasets/AmazonS3Object1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlob1c3')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureBlob",
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"fileName": "[parameters('AzureBlob1c3_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('AzureBlob1c3_properties_typeProperties_folderPath')]"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/AmazonS3Object1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"folder": {
					"name": "xxxx"
				},
				"type": "AmazonS3Object",
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": false
					},
					"key": "klu",
					"prefix": "",
					"bucketName": "xc"
				}
			}
		}
	]
}