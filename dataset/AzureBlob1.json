{
	"name": "AzureBlob1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"foldername": {
				"type": "String",
				"defaultValue": "xxx"
			}
		},
		"type": "AzureBlob",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": "",
			"folderPath": {
				"value": "@dataset().foldername",
				"type": "Expression"
			}
		}
	}
}