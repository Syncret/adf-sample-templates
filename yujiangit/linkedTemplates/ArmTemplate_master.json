{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "yujiangit"
		},
		"ADLS1fang_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'ADLS1fang'"
		},
		"AmazonS31_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS31'"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"AzureStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorage1'"
		},
		"FileShare_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileShare'"
		},
		"linkedService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedService1'"
		},
		"linkedService2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedService2'"
		},
		"ADLS1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().fileName"
		},
		"ADLS1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().filePath"
		},
		"ADLS1fang_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://fangldatalake.azuredatalakestore.net/webhdfs/v1"
		},
		"ADLS1fang_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "446db163-204e-4fa4-9a13-26846be6db21"
		},
		"ADLS1fang_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"ADLS1fang_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "eecfb6ee-86f1-45e1-bb7e-1f5b6bdf3951"
		},
		"ADLS1fang_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "ADMS_UX_TEST"
		},
		"AmazonS31_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "xx"
		},
		"AzureBlobStorage1_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureBlobStorage2'"
		},
		"FileShare_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\aa"
		},
		"FileShare_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "a"
		},
		"linkedService1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "xcv"
		},
		"linkedService1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "xcv"
		},
		"linkedService1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "zxcv"
		},
		"linkedService2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "xx"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS1fang_servicePrincipalKey": {
						"value": "[parameters('ADLS1fang_servicePrincipalKey')]"
					},
					"AmazonS31_secretAccessKey": {
						"value": "[parameters('AmazonS31_secretAccessKey')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"AzureStorage1_connectionString": {
						"value": "[parameters('AzureStorage1_connectionString')]"
					},
					"FileShare_password": {
						"value": "[parameters('FileShare_password')]"
					},
					"linkedService1_password": {
						"value": "[parameters('linkedService1_password')]"
					},
					"linkedService2_password": {
						"value": "[parameters('linkedService2_password')]"
					},
					"ADLS1fang_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('ADLS1fang_properties_typeProperties_dataLakeStoreUri')]"
					},
					"ADLS1fang_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ADLS1fang_properties_typeProperties_servicePrincipalId')]"
					},
					"ADLS1fang_properties_typeProperties_tenant": {
						"value": "[parameters('ADLS1fang_properties_typeProperties_tenant')]"
					},
					"ADLS1fang_properties_typeProperties_subscriptionId": {
						"value": "[parameters('ADLS1fang_properties_typeProperties_subscriptionId')]"
					},
					"ADLS1fang_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('ADLS1fang_properties_typeProperties_resourceGroupName')]"
					},
					"AmazonS31_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS31_properties_typeProperties_accessKeyId')]"
					},
					"AzureBlobStorage1_sasUri": {
						"value": "[parameters('AzureBlobStorage1_sasUri')]"
					},
					"AzureBlobStorage2_sasUri": {
						"value": "[parameters('AzureBlobStorage2_sasUri')]"
					},
					"FileShare_properties_typeProperties_host": {
						"value": "[parameters('FileShare_properties_typeProperties_host')]"
					},
					"FileShare_properties_typeProperties_userId": {
						"value": "[parameters('FileShare_properties_typeProperties_userId')]"
					},
					"linkedService1_properties_typeProperties_server": {
						"value": "[parameters('linkedService1_properties_typeProperties_server')]"
					},
					"linkedService1_properties_typeProperties_database": {
						"value": "[parameters('linkedService1_properties_typeProperties_database')]"
					},
					"linkedService1_properties_typeProperties_username": {
						"value": "[parameters('linkedService1_properties_typeProperties_username')]"
					},
					"ADLS1_properties_typeProperties_fileName": {
						"value": "[parameters('ADLS1_properties_typeProperties_fileName')]"
					},
					"ADLS1_properties_typeProperties_folderPath": {
						"value": "[parameters('ADLS1_properties_typeProperties_folderPath')]"
					},
					"linkedService2_properties_typeProperties_userName": {
						"value": "[parameters('linkedService2_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}